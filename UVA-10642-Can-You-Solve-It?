#include<iostream>
using namespace std ;

int value( int x, int y ) ;

int main () {
    int time, x1, y1, x2, y2 ;
    cin >> time ;
    for ( int i = 1 ; i <= time ; ++i ) {
        cin >> x1 >> y1 >> x2 >> y2 ;
        cout << "Case " << i << ": " << value( x2, y2 ) - value( x1, y1 ) << "\n" ;
    }
} 

int value( int x, int y ) {
    int v = ( x + y )*( x + y + 1 )/2 + 1 + x ;
    return v ;
}
